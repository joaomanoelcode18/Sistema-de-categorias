<h1>{{categoria.nome}}</h1>
{{#each postagens}}
<div class="card">
    <div class="card-body"> 
      
        <h3>{{titulo}}</h3>
        <p>{{descricao}}</p>
        <a href="/postagem/{{slug}}"><button class="btn btn-success">Leia mais</button></a>
        <hr>
      <small> Data de publicação:{{data}}</small>
       <hr>
       <h3>{{titulo}}</h3>
       <p>{{descricao}}</p>
    </div>
</div>
{{else}}
<H5>Nenhuma postagens</H5>

{{/each}}